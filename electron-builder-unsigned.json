  "mac": {
    "category": "public.app-category.accessibility",
    "identity": null,
    "icon": "icon.icns",
    "executableName": "Jarvis",
    "electronLanguages": ["en"],
    "extendInfo": {
      "CFBundleName": "Jarvis",
      "NSHumanReadableCopyright": "Accessibility support",
      "LSApplicationCategoryType": "public.app-category.accessibility",
      "LSUIElement": true
    },
    "target": [
      {
        "target": "dir",
        "arch": ["arm64"]
      }
    ]
  },
  "pkg": {
    "installLocation": "/Applications",
    "allowAnywhere": true,
    "allowRootDirectory": true,
    "allowCurrentUserHome": true
  },
  "dmg": {
    "sign": false,
    "contents": [
      {
        "x": 130,
        "y": 220,
        "type": "file"
      },
      {
        "x": 410,
        "y": 220,
        "type": "link",
        "path": "/Applications"
      }
    ],
    "window": {
      "width": 540,
      "height": 380
    }
  },
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": ["x64", "ia32"]
      }
    ],
    "icon": "icon.ico"
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true
  },
  "linux": null
}
